{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabled": true,
  "extends": [
    "local>adobe/spacecat-content-scraper"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "semantic-release"
      ],
      "allowedVersions": ">=23.0"
    },
    {
      "matchPackageNames": [
        "chai"
      ],
      "allowedVersions": "<5.0.0"
    },
    {
      "matchPackageNames": [
        "eslint"
      ],
      "allowedVersions": "<9.0.0"
    },
    {
      "matchPackageNames": [
        "@sparticuz/chromium"
      ],
      "allowedVersions": "121.0.0"
    }
  ]
}
